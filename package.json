    "react-is": "^19.2.0",
    "react-router-dom": "^7.9.4",
    "recharts": "^3.3.0",
    "sql.js": "^1.13.0",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@eslint/js": "^9.36.0",
