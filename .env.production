# Production environment variables
VITE_APP_TITLE="Options Trading Tracker"
VITE_APP_VERSION="0.1.0"
VITE_APP_DESCRIPTION="Track and analyze your options trading strategies"

# Production API
VITE_API_BASE_URL="https://api.optionstracker.com"
VITE_API_TIMEOUT=10000

# Production Feature Flags
VITE_ENABLE_DEBUG_MODE=false
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_PERFORMANCE_MONITORING=true

# Production Database
VITE_DB_NAME="options_tracker"
VITE_DB_VERSION=1

# Production CSV Settings
VITE_MAX_FILE_SIZE_MB=25
VITE_SUPPORTED_BROKERS="td_ameritrade,schwab,robinhood,etrade,interactive_brokers"

# Production Chart Settings
VITE_CHART_THEME="light"
VITE_CHART_ANIMATION_DURATION=300